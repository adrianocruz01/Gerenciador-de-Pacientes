generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Administrador {
  administrador_id Int       @id @unique(map: "administrador_id") @default(autoincrement())
  nome             String?   @db.VarChar(255)
  cpf              String    @unique @db.VarChar(14)
  senha            String    @db.VarChar(255)
  dtcadastro       DateTime  @default(now())
  dtalteracao      DateTime?
  status           String?   @db.Char(1)
}

model Colaborador {
  colaborador_id                         Int                                      @id @unique(map: "colaborador_id") @default(autoincrement())
  nome                                   String?                                  @db.VarChar(255)
  cpf                                    String                                   @unique @db.VarChar(14)
  senha                                  String?                                  @db.VarChar(255)
  especialidade                          String?                                  @db.VarChar(255)
  matricula                              String?                                  @db.VarChar(255)
  matricula_estado                       String?                                  @db.VarChar(255)
  dtcadastro                             DateTime?
  dtalteracao                            DateTime?
  status                                 String?                                  @db.Char(1)
  Ficha_Admissao_Paciente_Unidade        Ficha_Admissao_Paciente_Unidade[]
  Ficha_Avaliacao_Nutricional            Ficha_Avaliacao_Nutricional[]
  Ficha_Controle_Material                Ficha_Controle_Material[]
